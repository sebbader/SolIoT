#One Observation with multiple results containing all data previously held by json
<./> rdf:type sosa:Observation ;
    rdfs:label "Observations made by sensor 120636/sensor at 2018-10-24T01:22:30.866Z" ;
    sosa:madeBySensor <120636/GenericTrackerSensor> ;
    sosa:resultTime "2018-10-24T01:22:30.866Z"^^xsd:dateTime ;
    sosa:hasResult <./maximumTemperature> ;
    sosa:hasResult <./currentTemperature> ;
    sosa:hasResult <./minimumTemperature> ;
    sosa:hasResult <./lastAccelerometerMotionHistoryCount> ;
    sosa:hasResult <./lastMagnetometerMotionHistoryCount> ;
    sosa:hasResult <./lastAccelerometerMotionHistory> ;
    sosa:hasResult <./orientationChangeCount> ;
    sosa:hasResult <./reedActivationCount> ;
    sosa:hasResult <./lastMagnetometerMotionHistory> ;
    sosa:hasResult <./freeFallCount> ;
    idsSensor:reportReason "SCHEDULED" ;
    idsSensor:text "Tracker trigger activated" ;
    idsSensor:type "toil_TrackerEvent" .